{
  "name": "orbit-db-keystore",
  "version": "1.0.0",
  "description": "A local key manager for OrbitDB",
  "main": "src/keystore.js",
  "keywords": [
    "orbitdb",
    "orbit-db",
    "keystore",
    "kv-store"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbitdb/orbit-db-keystore.git"
  },
  "scripts": {
    "test": "npm run test:node && npm run test:browser",
    "test:node": "cross-env TEST=all mocha",
    "test:browser": "npm run build:tests && mocha-headless-chrome -f ./test/browser/index.html -a no-sandbox",
    "build": "npm run build:dist",
    "build:dist": "webpack --config ./conf/webpack.config.js --display-modules --sort-modules-by size --mode production",
    "build:tests": "webpack --config ./conf/webpack.tests.config.js --mode production",
    "lint": "standard"
  },
  "author": "Haad",
  "license": "MIT",
  "dependencies": {
    "elliptic": "^6.5.4",
    "level": "~7.0.0",
    "libp2p-crypto": "^0.21.2",
    "lru": "^3.1.0",
    "mkdirp": "^0.5.5",
    "reachdown": "^1.1.0",
    "safe-buffer": "^5.2.1",
    "secp256k1": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "babel-loader": "~8.0.4",
    "cross-env": "^7.0.2",
    "fs-extra": "^10.1.0",
    "is-node": "^1.0.2",
    "json-loader": "~0.5.7",
    "level-js": "^6.0.0",
    "level-js4": "npm:level-js@4",
    "leveldown": "~5.1.1",
    "levelup": "~4.1.0",
    "memdown": "^6.0.0",
    "mocha": "^10.0.0",
    "mocha-headless-chrome": "^4.0.0",
    "orbit-db-storage-adapter": "^0.7.0",
    "orbit-db-test-utils": "^1.0.3",
    "standard": "^17.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "bugs": "https://github.com/orbitdb/orbit-db-keystore/issues",
  "homepage": "https://github.com/orbitdb/orbit-db-keystore",
  "contributors": [
    "adam-palazzo",
    "haadcode",
    "RichardLitt",
    "shamb0t",
    "thiagodelgado111",
    "willemneal"
  ],
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "localMaintainers": [
    "haad <haad@haja.io>",
    "shamb0t <shams@haja.io>",
    "hajamark <mark@haja.io>"
  ]
}
